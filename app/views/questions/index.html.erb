<h3 id="question-block">All Questions</h3>
<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @questions.each do |question| %>
      <tr>
        <td id="question">-> <%= link_to question.title, "#{questions_url}/#{question.id}" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>

