<h3 id="question-block">All Questions</h3>
<table>
  <thead>
    <tr>
      <th colspan="7"></th>
    </tr>
  </thead>
  <tbody>
    <% @questions.each do |question| %>
      <tr>
        <td id="question">-> <%= link_to question.title, question_path(question) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<div style="padding-left: 800px">
<%= will_paginate @questions %>
</div>
<br>

